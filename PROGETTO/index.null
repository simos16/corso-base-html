}
blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}

/* Clearing */

.clear:before,
.clear:after,
.row:before,
.row:after,
.container:before,
.container:after {
	content: '';
	display: table;
}

.clear:after,
.row:after,
.container:after {
	clear: both;
}

