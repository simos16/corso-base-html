	max-width: 1000px;
	margin: auto;
	-webkit-transition: all 0.5s ease-in-out 0s;
	transition: all 0.5s ease-in-out 0s;
}

.row {
	margin: auto;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}

[class^="col-"] {
